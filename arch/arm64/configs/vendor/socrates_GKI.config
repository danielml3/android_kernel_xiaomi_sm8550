# Display
CONFIG_XIAOMI_DISP_NOTIFIER=m

# Fingerprint
CONFIG_FINGERPRINT_GOODIX_FOD=m
CONFIG_INPUT_FINGERPRINT=y

# IR
CONFIG_IR_SPI=m

# MTD
CONFIG_MTD=m
CONFIG_MTD_BLOCK=m
CONFIG_MTD_BLOCK2MTD=m
CONFIG_MTD_OOPS=m

# Power supply
CONFIG_MI_WIRELESS=y
CONFIG_WIRELESS_REVERSE_CLOSE=y

# Product
CONFIG_TARGET_PRODUCT_SOCRATES=y

# Touchscreen
CONFIG_TOUCHSCREEN_GOODIX_BRL_9916=m
CONFIG_TOUCHSCREEN_XIAOMI_TOUCHFEATURE=m

# WL2866D regulator
CONFIG_REGULATOR_WL2866D=m

# Xiaomi HWID
CONFIG_MI_HARDWARE_ID=m
